{
    "name": "nebula-new-tab-site",
    "version": "1.0.0",
    "private": true,
    "description": "Nebula New Tab - A clean and simple new tab page",
    "repository": {
        "type": "git",
        "url": "https://github.com/hkamran80/nebula-new-tab.git"
    },
    "scripts": {
        "build": "tailwindcss -i ./nebula-base.css -c ./tailwind.config.js -o ./nebula.css",
        "lint": "prettier --tab-width 4 --write ./**/*.{html,css,js,json}",
        "build:all": "npm run build; npm run lint",
        "prepare": "husky install"
    },
    "author": "H. Kamran",
    "license": "GPLV3",
    "devDependencies": {
        "autoprefixer": "^10.3.6",
        "husky": "^7.0.4",
        "prettier": "^2.4.1",
        "tailwind-scrollbar": "^1.3.1",
        "tailwindcss": "^2.2.16"
    }
}
